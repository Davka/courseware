<div class="cw-search-author-view">
    <div class="cw-search-description">
        <p>
            {{#i18n}}Beschreibung...{{/i18n}}
        </p>
    </div>
    <div class="cw-search-data">
        <ul>
            <li><label>{{#i18n}}Title{{/i18n}}:</label><input class="cw-search-input-title" type="text" value="{{searchtitle}}"></li>
        </ul>
    </div>
    <button name=save class=button>{{#i18n}}Speichern{{/i18n}}</button>
    <button name=cancel class=button>{{#i18n}}Abbrechen{{/i18n}}</button>
</div>
