<button class="cw-dialogcards-prev cw-dialogcards-navbutton"></button>
<div class="cw-dialogcards">
{{#cards}}
        <div class="scene scene--card" data-index="{{index}}">
          <div class="card">
            <div class="card__face card__face--front">
              <img src="{{front_img}}">
              <p>{{front_text}}</p>
            </div>
            <div class="card__face card__face--back">
            <img src="{{back_img}}">
              <p>{{back_text}}</p>
            </div>
          </div>
        </div>
{{/cards}}
</div>
<button class="cw-dialogcards-next cw-dialogcards-navbutton"></button>
