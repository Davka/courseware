<ul>
  {{#answers}}
    <li{{#correct_answer}} class="correct_answer"{{/correct_answer}}>
      {{#checked}}
        <img src="{{checked_image}}">
      {{/checked}}
      {{^checked}}
        <img src="{{unchecked_image}}">
      {{/checked}}

      <span class="answer">{{ text }}</span>
    </li>
  {{/answers}}
</ul>
