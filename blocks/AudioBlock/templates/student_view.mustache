<audio class="cw-audio-player">
    <source src="{{audio_file}}" type="audio/mpeg" />
</audio>
<div class="cw-audio-container">
    <p>{{audio_description}}</p>
    <div class="cw-audio-controls">
        <div class="cw-audio-range" ></div>
        <span class="cw-audio-time"></span>
        <button name="stop" class="cw-audio-stopbutton"></button>
        <button name="play" class="cw-audio-playbutton" played="{{audio_played}}"></button>
    </div>
</div>
